
package br.mackenzie.projeto.model;

public class Local {
    private String codLocal;
    private String nome;

    public Local() {
        this.codLocal = "";
        this.nome = "";
    }

    public String getCodLocal() {
        return codLocal;
    }

    public void setCodLocal(String codLocal) {
        this.codLocal = codLocal;
    }

    public String getNome() {
        return nome;
    }

    public void setNome(String nome) {
        this.nome = nome;
    }
    
    
}
